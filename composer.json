{
  "name": "pete/laravel-cloud-deploy-action",
  "type": "project",
  "description": "GitHub Action to deploy to Laravel Cloud",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
