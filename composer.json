{
  "name": "pete/laravel-cloud-deploy-action",
  "description": "CLI tool for deploying to Laravel Cloud",
  "require": {
    "php": "^8.2",
    "symfony/console": "^7.2",
    "guzzlehttp/guzzle": "^7.9",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "guzzlehttp/psr7": "^2.0"
  },
  "bin": [
    "bin/laravel-cloud-deploy"
  ],
  "autoload": {
    "psr-4": {
      "LaravelCloudDeploy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelCloudDeploy\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "minimum-stability": "stable"
}
